{
  "name": "zetoapi",
  "scripts": {
    "lint": "eslint",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && functions-framework --target=zetoapi",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run build && functions-framework --target=zetoapi",
    "deploy": "firebase deploy --only functions:zetoapi",
    "logs": "gcloud functions logs read --limit 50 --region=europe-west1"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.5.1",
    "@google-cloud/storage": "^7.16.0",
    "chance": "^1.1.12",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "multer": "^2.0.1",
    "nodemailer": "^6.10.0",
    "openai": "^5.16.0",
    "puppeteer": "^24.15.0",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.8.2"
  },
  "private": true
}